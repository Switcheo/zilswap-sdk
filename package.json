  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@types/websocket": "^1.0.0",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0"
  }
}
